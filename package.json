{
  "name": "harpia-nodejs",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "node -r dotenv/config ./dist/src/index.js dotenv_config_path=production.env",
    "start:dev": "ts-node-dev -r dotenv/config --respawn  --transpile-only --ignore-watch node_modules --quiet ./src/index.ts",
    "build": "tsc",
    "test": "jest --passWithNoTests",
    "make:resource": "ts-node -r dotenv/config --transpile-only ./core/functions/make-resource.ts",
    "seed:run": "ts-node ./node_modules/typeorm-seeding/dist/cli.js seed",
    "schema:drop": "ts-node ./node_modules/typeorm/cli.js schema:drop",
    "schema:sync": "ts-node ./node_modules/typeorm/cli.js schema:sync"
  },
  "devDependencies": {
    "@faker-js/faker": "^6.1.1",
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/faker": "^6.6.9",
    "@types/glob": "^7.2.0",
    "@types/jest": "^27.4.1",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^17.0.23",
    "@types/passport": "^1.0.7",
    "@types/passport-jwt": "^3.0.6",
    "@types/uuid": "^8.3.4",
    "jest": "^27.5.1",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "@types/joi": "^17.2.3",
    "@types/morgan": "^1.9.3",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "glob": "^7.2.0",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.4",
    "typeorm-pagination": "^2.0.3",
    "typeorm-seeding": "^1.6.1",
    "uuid": "^8.3.2"
  }
}
